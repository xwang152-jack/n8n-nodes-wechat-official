{
  "name": "n8n-nodes-wechat-official",
  "version": "1.0.0",
  "description": "n8n node for WeChat Official Account API integration. Supports access token management, material management, draft management and more WeChat Official Account features.",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "wechat",
    "wechat-official-account",
    "weixin",
    "api",
    "social-media",
    "messaging"
  ],
  "license": "MIT",
  "homepage": "https://github.com/xwang152-jack/n8n-nodes-wechat-official",
  "author": {
    "name": "xwang152-jack",
    "email": "xwagn152@163.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xwang152-jack/n8n-nodes-wechat-official.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes/**/*.ts credentials/**/*.ts package.json",
    "lintfix": "eslint nodes/**/*.ts credentials/**/*.ts package.json --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build && npm run lint && npm run test"
  },
  "files": [
    "dist",
    "credentials",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WechatOfficialAccountApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/WechatOfficialAccount/WechatOfficialAccount.node.js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^24.3.1",
    "@types/node-fetch": "^2.6.13",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "jest": "^29.5.0",
    "n8n-workflow": "^1.82.0",
    "node-fetch": "^3.3.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "axios": "^1.11.0"
  }
}
